{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": [
            "fixtures/",
            "./README.md"
        ]
    },
    "plugins": ["plugins/markdown"],
    "opts": {
        "encoding": "utf8",
        "template": "./",
        "destination": "./fixtures-doc/",
        "recurse": true,
        "verbose": true
    },
    "markdown": {
        "parser": "gfm",
        "hardwrap": true,
        "idInHeadings": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true,
            "includeDate": false
        }
    },
    "docdash": {
        "static": true,
        "sort": true,
        "disqus": "",
		"openGraph": {
			"title": "Fizz-Docdash",
			"type": "website",
			"site_name": "Fizz-Docdash",
			"url": "https://github.com/GuzmanPablo/fizz-docdash"
		},
		"meta": {
			"title": "Fizz-Docdash",
			"description": "Template para JSDoc 3 basado en Docdash para Fizzmod",
			"keyword": "jsdoc, docdash, fizzmod, fizz-docdash"
		},
        "search": true,
        "collapse": true,
        "typedefs": true,
        "removeQuotes": "none",
        "scripts": [],
        "menu":{
            "Repo Github":{
                "href":"https://github.com/GuzmanPablo/fizz-docdash",
                "target":"_blank",
                "class":"menu-item",
                "id":"repository"
            }
        }
    }
}
