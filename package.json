{
  "name": "fizz-docdash",
  "version": "0.1.0",
  "description": "Tema para JSDoc 3 inspirado en Docdash para Fizzmod Arg.s",
  "main": "publish.js",
  "scripts": {
    "test": "jsdoc -c fixtures/fixtures.conf.json",
    "sync": "browser-sync start -s ./fixtures-doc -f ./fixtures-doc --reload-delay 1000 --no-ui --no-notify",
    "watch": "watch-run -d 1000 -p tmpl/**,static/** \"npm run test\"",
    "start": "npm run test && npm run sync"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GuzmanPablo/fizz-docdash.git"
  },
  "devDependencies": {
    "jsdoc": "latest",
    "browser-sync": "latest",
    "watch-run": "latest"
  },
  "author": "Pablo E. Guzm√°n <guzman.pablo87@gmail.com>",
  "license": "Apache-2.0",
  "keywords": [
    "jsdoc",
    "template"
  ]
}
